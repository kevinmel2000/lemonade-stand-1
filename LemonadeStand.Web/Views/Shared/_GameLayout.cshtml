<!DOCTYPE html>
<html xmlns:fb="http://ogp.me/ns/fb#">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Game - Lemonade Stand</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <meta name="og:description" content="Family friendly Lemonade Stand business simulator." />
    <meta name="og:type" content="game" />
    <meta name="og:image" content="http://lemonade.aabech.no/content/images/share.jpg" />
</head>
<body class="sunny">
    <div id="fb-root"></div>
    <script>
        window.fbAsyncInit = function () {
            FB.init({
                appId: 230009193854410,
                status: false,
                xfbml: true
            });
        };

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/all.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>
    <div class="container body-content">
        @RenderBody()
    </div>
    <footer>
        <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
        <!-- Responsiv -->
        <ins class="adsbygoogle"
             style="display:block"
             data-ad-client="ca-pub-7982386306824762"
             data-ad-slot="2004977914"
             data-ad-format="auto"></ins>
        <script>
            (adsbygoogle = window.adsbygoogle || []).push({});
        </script>
        <div class="fb-like pull-right" data-href="http://lemonade.aabech.no" data-layout="standard" data-action="like" data-show-faces="false" data-share="false"></div>
        <p>
            &copy; @DateTime.Now.Year - Lars-Erik Aabech
        </p>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angularjs")
    @Scripts.Render("~/bundles/lemonade")
    <script src="~/Scripts/angular-slider.js"></script>
    @RenderSection("scripts", required: false)

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-50185657-1', 'aabech.no');
        ga('send', 'pageview');

    </script>
</body>
</html>
